<h2>Update the drone</h2>


<div>
{{!-- {{dronesUpdate}} --}}

  <form action="/drones/{{dronesUpdate._id}}/edit" method="post">

    <label for="name">Name</label>
    <input type="text" name="name" value="{{dronesUpdate.name}}" />
    <br />
    <label for="propellers">Propellers</label>
    <input type="Number" name="propellers" value="{{dronesUpdate.propellers}}"/>
    <br />
    <label for="maxSpeed">MaxSpeed</label>
    <input type="number" name="maxSpeed" value="{{dronesUpdate.maxSpeed}}"/>
    <br />
    <button>Update</button>
  </form>


</div>